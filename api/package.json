{
  "name": "mini-market-api",
  "version": "1.0.0",
  "description": "API for mini-market project",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config src/index.ts",
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js",
    "seed": "ts-node -r dotenv/config src/scripts/seed.ts",
    "seed:dev": "ts-node-dev src/scripts/seed.ts",
    "seed:require": "ts-node -e \"require('./src/scripts/seed.ts').seedWithRequire()\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.4.5",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0"
  }
}
